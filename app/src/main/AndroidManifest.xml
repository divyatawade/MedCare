    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.med_care">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Med_Care">
        <activity android:name=".hoslogin" />
        <activity android:name=".userview" />
        <activity android:name=".hos_reg" />
        <activity android:name=".adminview" />
        <activity android:name=".adminloginpage" />
        <activity android:name=".userlogin" />
        <activity android:name=".userReg" />
        <activity
